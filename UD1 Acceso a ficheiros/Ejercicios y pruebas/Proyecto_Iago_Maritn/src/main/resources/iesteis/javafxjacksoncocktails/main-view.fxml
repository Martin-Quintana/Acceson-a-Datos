<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="iesteis.javafxjacksoncocktails.controller.DrinkController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <AnchorPane prefHeight="375.0" prefWidth="560.0">
            <children>
                <Label layoutX="41.0" layoutY="14.0" text="Cocktails"/>
                <TextField fx:id="name" layoutX="101.0" layoutY="10.0"/>
                <Button fx:id="search" layoutX="271.0" layoutY="10.0" mnemonicParsing="false" onAction="#setName" text="Search"/>
                <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="339.0" layoutY="23.0" pickOnBounds="true"
                           preserveRatio="true"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
